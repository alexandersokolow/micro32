# initialize reg3 = 8, reg4 = 6

# reg2 = 1+1 
10000001000000010000000100000010

# reg2 = reg2 << 1
01000001000000100000000000000010

# reg2 = reg2 << 1
01000001000000100000000000000010

# reg3 = 1 + 1
10000001000000010000000100000011

# reg3 = reg3 + 1
10000001000000110000000100000011

# reg3 = reg3 << 1
01000001000000110000000000000011

# multiplication subroutine: reg5 = reg2 * reg3

# reg4 = reg3
00000001000000110000000000000100

# reg5 = 0
00000001000000000000000000000101

# JR = counter + 1
10010001000000000000000111111110

# reg5 = reg5 + reg2
10000001000001010000001000000101

# reg4 = reg4 + (-1)
10000001000001001111111100000100

# jump if reg4 != 0
00000110000001000000000000000000
