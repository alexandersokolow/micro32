# PROGRAM TO MULTIPLY REG2 = 4 AND REG3 = 3
# reg 2 and reg3 may be initialized to something else, but if this
# requires more/less commands, then the goto-statesments must be tweaked

# initialize reg2 = 4, reg3 = 3

# 0: reg2 = 1 << 1
00100000000100000010000000000000

# 1: reg2 = reg2 << 1
00100000001000000010000000000000

# 2: reg3 = 1 << 1
00100000000100000011000000000000

# 3: reg3 = reg3 + 1
01000000001100010011000000000000

# multiplication subroutine: reg4 = reg2 * reg3

# 4: reg4 = 0
00000000000000000100000000000000

# 5: reg5 = 1
00000000000100000101000000000000

# 6: reg5 = not reg5
11000000010100000101000000000000

# 7: reg5 = reg5 + 1
01000000010100010101000000000000

# 8: reg4 = reg4 + reg2
01000000010000100100000000000000

# 9: reg3 = reg3 + reg5 (= reg3 - 1)
01000000001101010011000000000000

# 10: if reg3 != 0 goto 12
00000001001100000000000000001100

# 11: goto 8
00000011000000000000000000001000

# 12: exit program
00000011000000000000111111111111
