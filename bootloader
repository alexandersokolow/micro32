# initialize reg3 = 8, reg4 = 6

# reg3 = 1 << 1
00100000001000000001000000000011

# reg3 = reg3 << 1
00100000001000000011000000000011
00100000001000000011000000000011

# reg4 = 1 << 1
00100000001000000001000000000100

# reg4 = reg4 + 1
01000000001000000001000100000100

# reg4 = reg4 << 1
00100000001000000100000000000100

# multiplication subroutine: reg5 = reg3 * reg4

# reg5 = 0
00000000001000000000000000000101

# cbuf = counter + 1
01000000000101000001000000000000

# reg5 = reg5 + reg3
01000000001000000011000101000101

# reg4 = reg4 + (-1)
01000000001000000100000010000100

# jump if reg4 != 0
00001010000000000100000000000000
